<Window x:Class="Login.StudentPreviousScores"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="StudentPreviousScores" Height="700" Width="1050" WindowStartupLocation="CenterScreen" Loaded="Window_Loaded_1">
    <Grid>
        <Grid.Background>
            <ImageBrush ImageSource="login_background.png"/>
        </Grid.Background>
        <Label Margin="200, 15, 200, 540" Content="State-Meant" Foreground="Khaki" FontStyle="Normal" FontWeight="ExtraBold" FontFamily="Old English Text MT" FontSize="80" RenderTransformOrigin="0.5,0.5" HorizontalAlignment="Center" >
            <Label.RenderTransform>
                <TransformGroup>
                    <ScaleTransform ScaleX="2"/>
                    <SkewTransform/>
                    <RotateTransform/>
                    <TranslateTransform/>
                </TransformGroup>
            </Label.RenderTransform>
        </Label>
        <Label x:Name="WelcomeUser" Margin="200, 120, 200, 480" FontSize="50" FontFamily="Edwardian Script ITC" FontWeight="Bold" FontStyle="Italic" Content="Welcome User" Foreground="Gold" HorizontalAlignment="Center" />
        <DataGrid x:Name="Report" Margin ="400, 300, 400, 200" AutoGenerateColumns="True" CanUserAddRows="False" CanUserDeleteRows="False" BorderBrush="Gold" />
        <Button x:Name="Back" Content="Back" Margin="314,518,561.2,117" Background="BurlyWood" Foreground="Blue" FontFamily="Comic Sans MS" FontSize="18" FontWeight="Bold" FontStyle="Italic" Click="Back_Click"/>
        <Button x:Name="Exit" Content="Exit" Margin="548,530,302.2,105" Background="BurlyWood" Foreground="Blue" FontFamily="Comic Sans MS" FontSize="18" FontWeight="Bold" FontStyle="Italic" Click="Exit_Click"/>
            
        
    </Grid>
</Window>
